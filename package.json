{
  "name": "@josephshambrook/js-2023",
  "version": "1.0.3",
  "private": true,
  "engines": {
    "node": "^16.14.2"
  },
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "lint:css": "stylelint \"**/*.{css,scss,astro,vue,svelte}\"",
    "lint:css:fix": "stylelint \"**/*.{css,scss,astro,vue,svelte}\"",
    "build": "astro build",
    "build:ci": "npm run build",
    "preview": "astro preview",
    "upgrade:deps": "npx npm-check-updates -i",
    "prepare": "husky install",
    "//": "had to disable pnpm as hit a few installation problems :(",
    "build:ci:pnpm": "npm install -g pnpm && pnpm i && pnpm build",
    "upgrade:pnpm": "pnpm add -g @pnpm/exe"
  },
  "husky": {
    "hooks": {
      "prepare-commit-msg": "exec < /dev/tty && npx cz --hook || true"
    }
  },
  "dependencies": {
    "@astrojs/image": "^0.16.6",
    "@astrojs/mdx": "^0.19.0",
    "@astrojs/partytown": "^1.2.0",
    "@astrojs/preact": "^2.1.0",
    "@astrojs/react": "^2.1.1",
    "@astrojs/rss": "^2.3.2",
    "@astrojs/sitemap": "^1.2.2",
    "@astrojs/svelte": "^2.1.0",
    "@vanilla-extract/css": "^1.11.0",
    "@vanilla-extract/vite-plugin": "^3.8.0",
    "astro": "^2.3.1",
    "autoprefixer": "^10.4.13",
    "bootstrap-icons": "^1.9.1",
    "classnames": "^2.3.2",
    "node-html-parser": "^6.1.5",
    "normalize.css": "^8.0.1",
    "preact": "^10.13.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rehype-autolink-headings": "^6.1.1",
    "rehype-slug": "^5.1.0",
    "remark-gfm": "^3.0.1",
    "remark-smartypants": "^2.0.0",
    "sass": "^1.62.1",
    "sharp": ">=0.32.0",
    "svelte": "^3.58.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.2.0",
    "@commitlint/config-conventional": "^17.2.0",
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/git": "^10.0.1",
    "@types/react": "^18.2.0",
    "husky": "^8.0.2",
    "postcss": "^8.4.23",
    "postcss-html": "^1.5.0",
    "prettier-plugin-astro": "^0.8.0",
    "semantic-release": "^19.0.5",
    "stylelint": "^14.14.1",
    "stylelint-config-html": "^1.1.0",
    "stylelint-config-prettier": "^9.0.4",
    "stylelint-config-prettier-scss": "^0.0.1",
    "stylelint-config-standard": "^29.0.0",
    "stylelint-config-standard-scss": "^6.1.0"
  }
}
