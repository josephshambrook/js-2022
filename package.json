{
  "name": "@josephshambrook/js-2023",
  "version": "1.1.0",
  "private": true,
  "engines": {
    "node": "^20.9.0"
  },
  "scripts": {
    "start": "astro dev",
    "dev": "astro check --watch & astro dev",
    "lint:css": "stylelint \"**/*.{css,scss,astro,vue,svelte}\"",
    "lint:css:fix": "stylelint \"**/*.{css,scss,astro,vue,svelte}\"",
    "build": "astro build",
    "build:ci": "npm run build",
    "preview": "astro preview",
    "upgrade:deps": "npx npm-check-updates -i --format group",
    "check:astro": "astro check",
    "check:tsc": "tsc --noEmit",
    "prepare": "husky install"
  },
  "husky": {
    "hooks": {
      "prepare-commit-msg": "exec < /dev/tty && npx cz --hook || true"
    }
  },
  "dependencies": {
    "@astrojs/check": "^0.2.1",
    "@astrojs/mdx": "^1.1.3",
    "@astrojs/partytown": "^2.0.1",
    "@astrojs/rss": "^3.0.0",
    "@astrojs/sitemap": "^3.0.2",
    "@astrojs/svelte": "^4.0.3",
    "@vanilla-extract/css": "^1.13.0",
    "@vanilla-extract/vite-plugin": "^3.9.0",
    "astro": "^3.4.0",
    "autoprefixer": "^10.4.16",
    "bootstrap-icons": "^1.11.1",
    "classnames": "^2.3.2",
    "node-html-parser": "^6.1.11",
    "normalize.css": "^8.0.1",
    "rehype-autolink-headings": "^7.0.0",
    "rehype-slug": "^6.0.0",
    "sass": "^1.69.5",
    "sharp": "^0.32.6",
    "svelte": "^4.2.2",
    "typescript": "^5.2.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.2.0",
    "@commitlint/config-conventional": "^18.1.0",
    "@csstools/postcss-global-data": "^2.1.0",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "husky": "^8.0.3",
    "postcss": "^8.4.31",
    "postcss-custom-media": "^10.0.2",
    "postcss-html": "^1.5.0",
    "postcss-import": "^15.1.0",
    "postcss-nesting": "^12.0.1",
    "prettier": "3.0.3",
    "prettier-plugin-astro": "^0.12.1",
    "prettier-plugin-svelte": "^3.0.3",
    "semantic-release": "^22.0.5",
    "stylelint": "^15.11.0",
    "stylelint-config-html": "^1.1.0",
    "stylelint-config-standard": "^34.0.0",
    "stylelint-config-standard-scss": "^11.0.0",
    "stylelint-prettier": "^4.0.2"
  }
}
