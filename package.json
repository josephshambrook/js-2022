    "@astrojs/svelte": "^1.0.2",
    "@vanilla-extract/css": "^1.9.1",
    "@vanilla-extract/vite-plugin": "^3.6.1",
    "astro": "^1.5.3",
    "bootstrap-icons": "^1.9.1",
