{
  "name": "@josephshambrook/js-2023",
  "version": "1.0.3",
  "private": true,
  "engines": {
    "node": "^18.16.0"
  },
  "scripts": {
    "start": "astro dev",
    "dev": "astro check --watch & astro dev",
    "lint:css": "stylelint \"**/*.{css,scss,astro,vue,svelte}\"",
    "lint:css:fix": "stylelint \"**/*.{css,scss,astro,vue,svelte}\"",
    "build": "astro build",
    "build:ci": "npm run build",
    "preview": "astro preview",
    "upgrade:deps": "npx npm-check-updates -i",
    "check:astro": "astro check",
    "check:tsc": "tsc --noEmit",
    "prepare": "husky install",
    "//": "had to disable pnpm as hit a few installation problems :(",
    "build:ci:pnpm": "npm install -g pnpm && pnpm i && pnpm build",
    "upgrade:pnpm": "pnpm add -g @pnpm/exe"
  },
  "husky": {
    "hooks": {
      "prepare-commit-msg": "exec < /dev/tty && npx cz --hook || true"
    }
  },
  "dependencies": {
    "@astrojs/image": "^0.17.3",
    "@astrojs/mdx": "^0.19.7",
    "@astrojs/partytown": "^1.2.3",
    "@astrojs/react": "^2.2.1",
    "@astrojs/rss": "^2.4.4",
    "@astrojs/sitemap": "^2.0.1",
    "@astrojs/svelte": "^3.1.0",
    "@vanilla-extract/css": "^1.12.0",
    "@vanilla-extract/vite-plugin": "^3.8.2",
    "astro": "^2.10.6",
    "autoprefixer": "^10.4.14",
    "bootstrap-icons": "^1.10.5",
    "classnames": "^2.3.2",
    "node-html-parser": "^6.1.5",
    "normalize.css": "^8.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rehype-autolink-headings": "^6.1.1",
    "rehype-slug": "^5.1.0",
    "sass": "^1.65.1",
    "sharp": "^0.32.4",
    "svelte": "^4.2.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.1",
    "@commitlint/config-conventional": "^17.6.1",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@types/react": "^18.2.20",
    "husky": "^8.0.3",
    "postcss": "^8.4.27",
    "postcss-custom-media": "^10.0.0",
    "postcss-html": "^1.5.0",
    "postcss-import": "^15.1.0",
    "prettier": "3.0.1",
    "prettier-plugin-astro": "^0.11.0",
    "semantic-release": "^21.0.1",
    "stylelint": "^15.6.0",
    "stylelint-config-html": "^1.1.0",
    "stylelint-config-standard": "^33.0.0",
    "stylelint-config-standard-scss": "^9.0.0"
  }
}
